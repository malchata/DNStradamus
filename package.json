{
  "name": "dnstradamus",
  "version": "1.2.0",
  "description": "A prognosticating client-side DNS prefetcher for outbound links.",
  "main": "dist/dnstradamus.cjs.js",
  "module": "dist/dnstradamus.es.mjs",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npx rollup -c",
    "lint": "npx eslint ./src/**.mjs",
    "test": "npm run lint && npm run build && node ./http.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/malchata/DNStradamus.git"
  },
  "keywords": [
    "rel=\"dns-prefetch\"",
    "dns-prefetch",
